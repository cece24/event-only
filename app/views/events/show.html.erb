<%= link_to "Show all events", events_path %>

<h1><%= @event.name %></h1>

<%= link_to "Edit Event", edit_event_path(@event) %> | <%= link_to "Delete Event", event_path(@event), method: :delete, data: {confirm: "Are you sure you want to delete #{ @event.name }? This cannot be undone!"} %>

<%= render 'one_event', event: @event %>
